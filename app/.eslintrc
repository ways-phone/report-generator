{
  "rules": {
    "strict": [2, "never"],
    "require-jsdoc": 0,
    "no-invalid-this": 0,
    "max-len": [1, 240],
    "object-curly-spacing": ["error", "always"],
    "arrow-parens": "off",
    "func-call-spacing": "off",
    "space-before-function-paren": "off",
    "babel/new-cap": 1,
    "babel/no-await-in-loop": 1,
    "linebreak-style": "off",
    "quotes": "error"
  }
}
